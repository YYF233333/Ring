; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Ring Engine"
run/main_scene="res://breakout/scenes/breakout/breakout.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

BreakoutManager="*res://breakout/scenes/managers/breakout_manager.gd"
DropManager="*res://breakout/scenes/managers/drop_manager.gd"
ValueManager="*res://breakout/scenes/managers/value_manager.gd"
Utility="*res://breakout/scenes/utility/utility.gd"

[debug]

gdscript/warnings/unused_parameter=0

[display]

window/size/viewport_width=1600
window/size/viewport_height=900
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="RingEngine"

[editor]

export/convert_text_resources_to_binary=false

[filesystem]

import/blender/enabled=false

